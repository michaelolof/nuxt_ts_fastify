{
  "name": "nuxt-fastify-baremetal-starterpack",
  "private": true,
  "type": "module",
  "imports": {
    "#server/*": "./server/*.ts",
    "#utils/*": "./utils/*.ts"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "start": "node .output/server/index.mjs",
    "build:start": "nuxt build && npm run start",
    "db:migrate:generate": "pnpm exec drizzle-kit generate --config ./drizzle.config.ts",
    "db:migrate:run": "node --loader ts-node/esm --experimental-specifier-resolution=node ./server/db/migrations.ts",
    "db:migrate": "npm run db:migrate:generate && npm run db:migrate:run",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@sinclair/typebox": "^0.32.33",
    "close-with-grace": "^1.3.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "fastify": "^4.28.0",
    "nuxt": "^3.12.2",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.1",
    "postgres": "^3.4.4",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@nuxt/test-utils": "^3.13.1",
    "@stylistic/eslint-plugin": "^2.2.1",
    "@vue/test-utils": "^2.4.6",
    "drizzle-kit": "^0.22.7",
    "eslint": "^9.5.0",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.6.0",
    "happy-dom": "^14.12.3",
    "playwright-core": "^1.44.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20.9.0"
  }
}